<?xml version="1.0"?>
<object name="portal_css">

  <!-- Set conditions on stylesheets we don't want to pull in -->
  <stylesheet
    expression=""
    id="public.css"
    rendering="link"
    />

  <stylesheet
    expression=""
    id="columns.css"
    rendering="link"
    />

  <stylesheet
    conditionalcomment="lt IE 8"
    expression=""
    id="IEFixes.css"
    rendering="link"
    />

  <stylesheet
    expression=""
    id="member.css"
    rendering="link"
    />

  <stylesheet
    expression=""
    id="mobile.css"
    rendering="link"
    />

  <stylesheet
    expression=""
    id="portlets.css"
    rendering="link"
    />

  <stylesheet
    expression=""
    id="print.css"
    rendering="link"
    />

  <stylesheet
    expression="python:portal.restrictedTraverse('@@plone_portal_state').is_rtl()"
    id="RTL.css"
    rendering="link"
    />

  <stylesheet
    expression=""
    id="base.css"
    rendering="link"
    />

  <stylesheet
    expression=""
    id="authoring.css"
    rendering="link"
    />

  <stylesheet
    expression=""
    id="controlpanel.css"
    rendering="link"
    />

  <stylesheet
    expression=""
    id="deprecated.css"
    rendering="link"
    />

  <stylesheet
    expression=""
    id="navtree.css"
    rendering="link"
    />

  <stylesheet
    expression=""
    id="invisibles.css"
    rendering="link"
    />

  <stylesheet
    expression=""
    id="forms.css"
    rendering="link"
    />

  <stylesheet
    applyPrefix="True"
    authenticated="False"
    cacheable="True"
    compression="safe"
    conditionalcomment=""
    cookable="True"
    enabled="on"
    expression="request/HTTP_X_THEME_ENABLED | nothing"
    id="++theme++sll.theme/css/main.css"
    media="screen"
    rel="stylesheet"
    rendering="link"
    title=""
    />

  <stylesheet
    applyPrefix="True"
    authenticated="False"
    cacheable="True"
    compression="safe"
    conditionalcomment=""
    cookable="True"
    enabled="on"
    expression="request/HTTP_X_THEME_ENABLED | nothing"
    id="++theme++sll.theme/css/extra.css"
    media="screen"
    rel="stylesheet"
    rendering="link"
    title=""
    />

</object>
